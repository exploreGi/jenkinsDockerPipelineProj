
pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'openjdk:11' }
      }
      steps {
        sh 'javac HelloJenkins.java'
        sh 'java HelloJenkins'
      }
    }
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'node --version'
      }
    }
  }
}
